<layout version="0.1.0">
    <adminhtml_celexport_debug_index>
        <reference name="head">
            <action method="addCss"><stylesheet>css/celebros/celexport.css</stylesheet></action>
        </reference>
    </adminhtml_celexport_debug_index>
    <adminhtml_celexport_debug_grid>
        <block type="core/text_list" name="root" output="toHtml">
            <block type="celexport/adminhtml_debug_cron_tasks" name="cron.tasks.grid"/>
        </block>
    </adminhtml_celexport_debug_grid>
    <adminhtml_celexport_debug_systemcronlog>
        <block type="core/text_list" name="root" output="toHtml">
            <block type="celexport/adminhtml_debug_cron_cron" name="system.cron.log.grid"/>
        </block>
    </adminhtml_celexport_debug_systemcronlog>
</layout>